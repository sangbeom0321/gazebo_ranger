  <!-- MACRO -->

  <xacro:macro name="box_inertia" params="m x y z">
      <inertia ixx="${m * (y * y + z * z) / 12}" ixy="0" ixz="0" iyy="${m * (x * x + z * z) / 12}" iyz="0" izz="${m * (y * y + x * x) / 12}" />
  </xacro:macro>

  <!-- LIDAR -->
  <xacro:include filename="$(find gazebo_ranger)/urdf/sensors/vlp-32.urdf.xacro"/>
  <xacro:VLP-32 >
    <origin xyz="0 0 0" rpy="-1.57 -1.57 0.0" />
  </xacro:VLP-32> 

  <!-- CAMERA -->
  <xacro:include filename="$(find gazebo_ranger)/urdf/sensors/kinect_camera.xacro" />
  <xacro:kinect_camera camera_key="camera" />

  <!-- IMY -->
  <xacro:include filename="$(find gazebo_ranger)/urdf/sensors/imu.xacro" />
  <xacro:imu />

  <!-- GPS -->
  <xacro:include filename="$(find gazebo_ranger)/urdf/sensors/gps.xacro" />
  <xacro:gps />


</robot>

