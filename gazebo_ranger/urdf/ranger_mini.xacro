      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/camera_link.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="camer_joint"
    type="fixed">
    <origin
      xyz="-0.0029998 -0.17935 0.19625"
      rpy="1.5708 0 0" />
    <parent
      link="ranger_base" />
    <child
      link="camera_link" />
    <axis
      xyz="0 0 0" />
  </joint>

  <link
    name="imu_link">
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/imu_link.STL" />
      </geometry>
      <material
        name="">
        <color
