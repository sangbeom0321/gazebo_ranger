      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/imu_link.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="imu_joint"
    type="fixed">
    <origin
      xyz="-0.003 0.087717 0.25475"
      rpy="-1.5708 0 -1.5708" />
    <parent
      link="ranger_base" />
    <child
      link="imu_link" />
    <axis
      xyz="0 0 0" />
  </joint>

   <link name="fl_steering_wheel_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0"/>
            <mass value="1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
