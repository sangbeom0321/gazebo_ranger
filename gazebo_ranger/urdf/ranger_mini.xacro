<?xml version="1.0" encoding="utf-8"?>

<robot
  name="ranger_mini_v2" xmlns:xacro="http://ros.org/wiki/xacro">

  <xacro:include filename="$(find gazebo_ranger)/urdf/ranger_l.xacro" />
  <xacro:include filename="$(find gazebo_ranger)/urdf/ranger_r.xacro" />
  <xacro:include filename="$(find gazebo_ranger)/urdf/sensors/kinect_camera.xacro" />

  <!-- Variables -->
  <xacro:property name="M_PI" value="3.14159"/>

  <!-- Vehicle Geometries -->
  <xacro:property name="base_x_size" value="1.0" />
  <xacro:property name="base_y_size" value="0.380000" />
  <xacro:property name="base_z_size" value="0.210000" />

  <xacro:property name="wheelbase" value="0.498" />
  <xacro:property name="track" value="0.58306" />
  <xacro:property name="wheel_vertical_offset" value="-0.0702" />

  <xacro:property name="wheel_length" value="1.1653e-01" />
  <xacro:property name="wheel_radius" value="1.6459e-01" />

  <link name="base_link">
      <inertial>
          <origin xyz="0.0 0.0 0.0"/>
          <mass value="50"/>
          <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
      </inertial>
      <visual>
          <origin xyz="0 0 0" rpy="0 0 0" />
          <geometry>
            <box size="0.5 0.0001 0.2"/>
          </geometry>
          <material
            name="">
            <color
            rgba="0.784 0.784 0.784 1" />
      </material>
      </visual>

  </link>

  <link
    name="ranger_base">
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.784 0.784 0.784 1" />
      </material>
    </visual>
  </link>

  <joint
    name="base_to_body_joint"
    type="fixed">
    <origin
      xyz="0.2 0.0 -0.1"
      rpy="0 0 1.57" />
    <parent
      link="base_link" />
    <child
      link="ranger_base" />
    <axis
      xyz="0 0 0" />
  </joint>

  <link
    name="lidar_connector_link">
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/lidar_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.314 0.314 0.314 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/lidar_link.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="lidar_joint"
    type="fixed">
    <origin
      xyz="-0.003125 0.020748 0.40825"
      rpy="1.5708 0 0" />
    <parent
      link="ranger_base" />
    <child
      link="lidar_connector_link" />
    <axis
      xyz="0 0 0" />
  </joint>
  <link
    name="camera_link">
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find gazebo_ranger)/meshes/ranger_mini_v2/camera_link.STL" />
      </geometry>
      <material
        name="">
        <color
